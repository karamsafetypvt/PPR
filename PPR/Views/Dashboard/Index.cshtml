                                                foreach (var leve12 in Model._DevelopmentSheetLevelList)
                                                {
                                                    <tr class="odd gradeX">
                                                        @if (leve12.Level == "2")
                                                        {
                                                            <td>@m</td>
                                                            <td>
                                                                @leve12.UserName 
                                                            </td>
                                                            <td><span class="badge badge-info" style="margin-left: 53%;">@leve12.LevelTwoCount</span></td>
                                                            <input type="hidden" value="@(++j)" />

                                                        }
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>


                        @*<div class="span4">
                            <div class="widget-box">
                                <div class="widget-content nopadding">
                                    <table class="table table-bordered table-striped">
                                        <thead>
                                            <tr>
                                                <th colspan="3">Level 3</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Model._DevelopmentSheetLevelList != null)
                                            {
                                                foreach (var leve13 in Model._DevelopmentSheetLevelList)

                                                {
                                                    <tr class="odd gradeX">
                                                        @if (leve13.Level == "3")
                                                        {
                                                            <td>@n</td>
                                                            <td>
                                                                @leve13.UserName 
                                                            </td>
                                                            <td>
                                                                <span class="badge badge-info" style="margin-left: 49%;">@leve13.LevelThreeCount</span>
                                                            </td>
                                                            <input type="hidden" value="@(++k)" />

                                                        }
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@*<script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/maruti.dashboard.js"></script>
    <script src="~/Content/js/maruti.form_common.js"></script>*@